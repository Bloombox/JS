{
  "hosting": {
    "public": "public",
    "headers": [{
        "source": "client/**/*.js",
        "headers": [{
            "key": "Cache-Control",
            "value": "public, max-age=31536000, no-transform, immutable, stale-while-revalidate=86400, stale-if-error=86400"}]
    }, {
      "source": "*.min.js",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=500, s-max-age=3600, no-transform"}]
    }],
    "redirects": [{
      "source" : "/latest.min.js",
      "destination" : "/client/v2.0.0.min.js",
      "type" : 302
    },{
      "source" : "/latest-debug.min.js",
      "destination" : "/client/v2.0.0-debug.min.js",
      "type" : 302
    },{
      "source" : "/beta.min.js",
      "destination" : "/client/v2.0.0.min.js",
      "type" : 302
    },{
      "source" : "/beta-debug.min.js",
      "destination" : "/client/v2.0.0-debug.min.js",
      "type" : 302
    }]
  }
}
